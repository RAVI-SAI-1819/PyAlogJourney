   "source": [
    "def reverseArray(a):\n",
    "    left, right = 0, len(a) -1\n",
    "    while left < right:\n",
    "        a[left], a[right] = a[right] , a[left]\n",
    "        left += 1\n",
    "        right -= 1\n",
    "    return a"
   ]
